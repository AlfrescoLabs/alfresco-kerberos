Alfresco {
  com.sun.security.auth.module.Krb5LoginModule sufficient;
};
   
AlfrescoHTTP
{
  com.sun.security.auth.module.Krb5LoginModule required
    storeKey=true
    useKeyTab=true
    doNotPrompt=true
    keyTab="/etc/keytabs/alfresco.keytab"
    principal="HTTP/alfresco@EXAMPLE.COM";
};
          
  com.sun.net.ssl.client {
    com.sun.security.auth.module.Krb5LoginModule sufficient;
};
          
other {
  com.sun.security.auth.module.Krb5LoginModule sufficient;
};
